    "paths": {
        "/send": {
            "post": {
                "tags": [
                    "Notifications"
                ],
                "summary": "Notifications",
                "description": "Send Notification",
                "operationId": "NotifEmail_",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "in": "body",
                        "name": "body",
                        "description": "",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "properties": { 
                                "to": {
                                    "type": "string",
                                    "example": "lutfi.f.hidayat@gmail.com"
                                },
                                "cc": {
                                    "type": "string",
                                    "example": ""
                                },
                                "subject": {
                                    "type": "string",
                                    "example": "Subject"
                                },
                                "body": {
                                    "type": "string",
                                    "example": "Body"
                                },
                                "type": {
                                    "type": "string",
                                    "example": "email"
                                },
                                "attachment": {
                                    "type": "string",
                                    "example": ""
                                },
                            },
                            "required": ["to", "subject","body","type","attachment"],
                            "$schema": "http://json-schema.org/draft-04/schema#"
                        }
                    }],
                "responses": {
                    "200": {
                        "description": ""
                    }
                },
                "security": []
            },
        },



        "/check-token": {
            "get": {
                "summary": "Check token",
                "responses": {
                    "200": {
                        "description": "Get user with token"
                    },
                    "default": {
                        "description": "an \"unexpected\" error"
                    }
                }
            }
        }
    },
